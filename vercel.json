{
  "functions": {
    "src/app/api/auto-complete/**": {
      "maxDuration": 30
    },
    "src/app/api/chapters/**": {
      "maxDuration": 45
    },
    "src/app/api/generate/**": {
      "maxDuration": 30
    },
    "src/app/api/book-bible/**": {
      "maxDuration": 30
    },
    "src/app/api/quality/**": {
      "maxDuration": 60
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ]
}